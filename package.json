{
  "name": "vscode-copy-tabs-filepath",
  "displayName": "vscode-copy-tabs-filepath",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-copy-tabs-filepath.helloWorld",
        "title": "vscode-copy-tabs-filepath"
      },
      {
        "command": "vscode-copy-tabs-filepath.copyFilePath",
        "title": "copy file info"
      }
    ],
    "menus": {
      "editor/title/context": [
        {
          "command": "vscode-copy-tabs-filepath.copyFilePath",
          "when": "resourceLangId != ''",
          "group": "navigation@100"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
