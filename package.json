{
  "name": "vscode-copy-tabs-filepath",
  "displayName": "vscode-copy-tabs-filepath",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "scripts": {
    "lint": "eslint --fix ./src",
    "pack": "npx -y vsce package && code --install-extension vscode-copy-tabs-filepath-0.1.0.vsix",
    "": ""
  },
  "contributes": {
    "commands": [
      {
        "command": "vscode-copy-tabs-filepath.copyTabsFilePath",
        "title": "Copy Tabs FilePath"
      }
    ],
    "menus": {
      "editor/title/context": [
        {
          "command": "vscode-copy-tabs-filepath.copyTabsFilePath",
          "when": "resourceLangId != ''",
          "group": "navigation@100"
        }
      ]
    }
  },
  "devDependencies": {
    "eslint": "^9.25.1"
  }
}
